  "query": "SELECT gateway.*, u.first_name modified_by_firstname, u.last_name modified_by_lastname, CASE WHEN gateway.connected_at IS NULL THEN false WHEN gateway.disconnected_at IS NULL THEN true WHEN gateway.connected_at >= gateway.disconnected_at THEN true ELSE false END AS \"connected!\", wn.name AS location_name FROM gateway JOIN \"user\" u on gateway.modified_by = u.id JOIN wireguard_network wn ON gateway.location_id = wn.id WHERE location_id = $1",
